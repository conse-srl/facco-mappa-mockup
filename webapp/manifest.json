{
    "_version": "1.12.0",
    "sap.app": {
        "id": "openui5-calendar",
        "type": "application",
        "dataSources": {}
    },
    "sap.ui5": {
        "handleValidation": true,
        "dependencies": {
            "minUI5Version": "1.108.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.unified": {}
            }
        },
        "rootView": {
            "viewName": "openui5-calendar.view.Home",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "async": true,
                "settings": {
                    "bundleName": "openui5-calendar.i18n.i18n",
                    "supportedLocales": [
                        "en",
                        "it"
                    ],
                    "fallbackLocale": "en"
                }
            },
            "themes": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/themes.json",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/styles.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "openui5-calendar.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "transition": "show",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "home",
                    "target": "home"
                }
            ],
            "targets": {
                "home": {
                    "viewId": "home",
                    "viewName": "Home"
                }
            }
        }
    }
}